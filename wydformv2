		bm.Scale=bm.Scale:Lerp(bm.Scale+Vector3.new(.23,.23,.23),.3)
		bm2.Scale=bm2.Scale:Lerp(bm2.Scale+Vector3.new(.3,.3,.3),.3)
		end
		local Ring=part(Char,true,false,v3(0,0,0),ball.CFrame,c3(0,0,0),"Neon",.5)
