table.insert(CurrentEffects,MagicWeld)
for i=1,40 do
	MagicWeld.C0=MagicWeld.C0*angles(rad(6),0,0)
	CirM.Scale=CirM.Scale+v3(0,.4,.4)
	CirDec.Transparency=i/40
	r:wait()
end
