BodyParts = {}
for _, v in pairs(Character:GetChildren()) do
  if v:IsA("Part") then
    table.insert(BodyParts, v)
  end
end
Bounding = {}
for _, v in pairs(BodyParts) do
  local temp = {
    X = nil,
    Y = nil,
    Z = nil
  }
  temp.X = v.Size.X / 2 * 35
  temp.Y = v.Size.Y / 2 * 35
  temp.Z = v.Size.Z / 2 * 35
  Bounding[v.Name] = temp
end
