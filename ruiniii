      Snow(maincolor, Torso.CFrame * cn(math.random(-10, 10), math.random(5, 7), math.random(-10, 10)), 0.3, 0.2, 0.3, 0, 0, 0, 0.01)
      ImageLabel1.ImageTransparency = 0.5
    else
      ImageLabel1.ImageTransparency = 1
    end
  end
end))
