local FavIDs = {340106355,927529620,876981900,398987889,1117396305}

function swait(num)
if num==0 or num==nil then
game:service'RunService'.Stepped:wait(0)
else
for i=0,num do
game:service'RunService'.Stepped:wait(0)
end
end
end
function thread(f)
  coroutine.resume(coroutine.create(f))
end

