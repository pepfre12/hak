end
if root.Velocity.y > 1 and hitfloor==nil then 
Anim="Jump"
if attack==false then
rootj.C0 = clerp(rootj.C0,RootCF*cf(0,0,-0.1+0.1*math.cos(sine/20))* angles(math.rad(10),math.rad(0),math.rad(0)),.3)
