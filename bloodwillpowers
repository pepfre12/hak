MANATEXT.Text = "ENERGY ["..FLOOR(MANA).."]"
if MANA >= MAXMANA then
	MANACOVER.Transparency = 1
	SKILL3TEXT.TextTransparency = 0
else
	MANACOVER.Transparency = 0
	SKILL3TEXT.TextTransparency = 1
end
end

