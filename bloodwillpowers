--//=================================\\
--||	ATTACK FUNCTIONS AND STUFF
--\\=================================//

function clerp(a, b, t)
  return a:lerp(b, t)
end

function newBezier(startpos, pos2, pos3, endpos, t)
  local A = clerp(startpos, pos2, t)
  local B = clerp(pos2, pos3, t)
  local C = clerp(pos3, endpos, t)
  local lerp1 = clerp(A, B, t)
  local lerp2 = clerp(B, C, t)
  local cubic = clerp(lerp1, lerp2, t)
  return cubic
end

function BloodArrow(Target)
	ATTACK = true
	local newball = Instance.new("Part", workspace)
	newball.Anchored = true
	newball.Shape = "Ball"
	newball.Material = "Neon"
	newball.Size = Vector3.new(0,0,0)
	newball.CanCollide = false
	newball.CFrame = CF(Target)
	newball.BrickColor = BRICKC"Really red"
	newball.Transparency = 1
	local POS = Mouse.Hit.p
	local onefourth = Target:Lerp(POS, 0.25) + Vector3.new(0,0,0)
	local threefourths = Target:Lerp(POS, 0.75) + Vector3.new(0,0,0)
	 local A1 = IT("Attachment",newball)
	    A1.Position = Vector3.new(0, 0.5, 0)
	  local A2 = IT("Attachment",newball)
	    A2.Position = Vector3.new(0, -0.5, 0)
	  local Trail = IT("Trail",newball)
	    Trail.LightEmission = 1
	    Trail.FaceCamera = true
	    Trail.Texture = "rbxassetid://945758042"
	    Trail.Attachment0 = A1
	    Trail.Attachment1 = A2
	    Trail.Lifetime = 2
	    Trail.MinLength = 0
	    Trail.Transparency = NumberSequence.new(0, 1)
	    Trail.Color = ColorSequence.new(newball.Color)
  	coroutine.resume(coroutine.create(function()
   		for i = 0, 1, 0.07 do
    	  	Swait()
    	  	newball.CFrame = CFrame.new(newBezier(Target, onefourth, threefourths, POS, i))
    	end
		newball.Transparency = 1
		CreateSound("514867425", newball, 3, 1)
		AoEDamage(newball.Position,5,15,25,5,false,2,2,false,false)
		MagicSphere(VT(22,22,22),55,newball.CFrame,"Really red",VT(0.2,0.2,0.2))
		MagicSphere(VT(20,20,20),55,newball.CFrame,"Pearl",VT(0.2,0.2,0.2))
  		game:GetService("Debris"):AddItem(newball, 5)
	end))
	ATTACK = false
end

function BloodPillar()
	ATTACK = true
  	coroutine.resume(coroutine.create(function()
		local CFRAME = CF(Mouse.Hit.p)
		MagicSphere(VT(15,6500,15),55,CF(Mouse.Hit.p),"Really red",VT(-15/55,0,-15/55))
		MagicSphere(VT(14,6500,14),55,CF(Mouse.Hit.p),"Pearl",VT(-14/55,0,-14/55))
		Swait(55)
		AoEDamage(Mouse.Hit.p,5,15,25,5,false,2,2,false,false)
		local aaaa = MagicSphere(VT(22,22,22),55,CFRAME,"Really red",VT(0.2,0.2,0.2))
		MagicSphere(VT(20,20,20),55,CFRAME,"Pearl",VT(0.2,0.2,0.2))
		CreateSound("514867425", aaaa, 3, 1)
	end))
	ATTACK = false
end

function Violence()
	ATTACK = true
  	coroutine.resume(coroutine.create(function()
		local CFRAME = CF(Mouse.Hit.p)
		MagicSphere(VT(35,6500,35),155,CF(Mouse.Hit.p),"Really red",VT(-35/155,0,-35/155))
		MagicSphere(VT(34,6500,34),155,CF(Mouse.Hit.p),"Pearl",VT(-34/155,0,-34/155))
		Swait(155)
		AoEDamage(Mouse.Hit.p,65,1500,2500,45,false,2,2,false,false)
		local aaaa = MagicSphere(VT(62,62,62),155,CFRAME,"Really red",VT(2,2,2))
		MagicSphere(VT(60,60,60),155,CFRAME,"Pearl",VT(2,2,2))
		CreateSound("514867425", aaaa, 16, 0.85)
		MANA = 0
	end))
	ATTACK = false
end

