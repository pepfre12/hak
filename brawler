Torso.Neck.C0 = clerp(Torso.Neck.C0,necko *angles(math.rad(-4+8*math.cos(sine/18)),math.rad(0),math.rad(0) + RootPart.RotVelocity.Y / 13),.2)
RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(20*math.cos(sine/18))+ RootPart.RotVelocity.Y / 34, math.rad(0), math.rad(10)+ RootPart.RotVelocity.Y / 34),.2)
LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(-20*math.cos(sine/18))+ RootPart.RotVelocity.Y / 34, math.rad(0), math.rad(-10)- RootPart.RotVelocity.Y / -34),.2)
end
if attack==false then
LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.07*math.cos(sine/15), 0+0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0-20*math.sin(sine/15)), math.rad(3), math.rad(0)), 0.1)
RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.07*math.cos(sine/15), 0-0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0+20*math.sin(sine/15)), math.rad(-3), math.rad(0)), 0.1)
end
if attack==true and noleg == false then
LeftLeg.Weld.C0 = clerp(LeftLeg.Weld.C0, CFrame.new(-0.5, -1-0.07*math.cos(sine/15), 0+0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0-20*math.sin(sine/15)), math.rad(3), math.rad(0)), 0.1)
RightLeg.Weld.C0 = clerp(RightLeg.Weld.C0, CFrame.new(0.5, -1+0.07*math.cos(sine/15), 0-0.04*math.sin(sine/15)) * CFrame.Angles(math.rad(0+20*math.sin(sine/15)), math.rad(-3), math.rad(0)), 0.1)
end
