function kfblock()
attack = true
Humanoid.WalkSpeed = 0
for i=0,1,.2 do
swait()
PlayAnimationFromTable({
CFrame.new(0, 0, 1.90734863e-06, 0, 0, -1, 0, 1, 0, 1, 0, 0),
CFrame.new(-1.90734863e-06, 1.49999642, 0, 0, 0, 1, 0, 1, 0, -1, 0, 0),
CFrame.new(0.29999733, 0, -0.799991608, 0, 1, 0, 0, 0, -1, -1, 0, 0),
CFrame.new(-1.5, 0.999999762, 0, 1, 0, 0, 0, -1, 0, 0, 0, -1),
CFrame.new(0.500000238, -2, 2.38418579e-07, 0.866025388, 0, -0.500000238, 0, 1, 0, 0.500000238, 0, 0.866025388),
CFrame.new(-0.5, -2, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1),
}, .3, false)
end
local oldhp = Humanoid.Health
repeat
swait()
if Humanoid.Health < oldhp then
so(260430060,Torso,math.random(9,11)/10,1)
local vp = Instance.new("BodyVelocity", Torso)
vp.MaxForce = Vector3.new(4800000, 0, 4800000)
vp.P = 200000
vp.Velocity = RootPart.CFrame.lookVector * -30
game:GetService("Debris"):AddItem(vp,.01)
end
